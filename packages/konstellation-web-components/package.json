{
  "name": "konstellation-web-components",
  "version": "1.6.5",
  "description": "Components library for Konstellation projects",
  "author": "konstellation-io",
  "license": "MIT",
  "repository": "konstellation-io/kwc",
  "main": "src/index.tsx",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl -o dist/index.js --format modern,cjs && cp -R ./src/styles ./dist && cp ./.npmignore ./dist && node ../../scripts/copy-files.js",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "release": "yarn build && npm publish dist --tag latest",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prettier": "prettier --write --single-quote \"src/**/*.(ts|tsx)\"",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "lodash": "^4.17.19",
    "react-dates": "^21.8.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/d3-array": "^2.0.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.158",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dates": "^17.1.13",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "d3-array": "^2.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "3.0.9",
    "jest-canvas-mock": "^2.2.0",
    "lottie-web": "^5.7.0",
    "microbundle-crl": "^0.13.10",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typeface-montserrat": "^0.0.75",
    "typeface-roboto-mono": "^0.0.75",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
